source ~/.bashrc
tmux has-session -t dev
if [ $? != 0 ]; then
reload-ssh-agent
tmux new-session -s dev -n shell -d

tmux new-window -n mail -t dev
tmux send-keys -t dev:2 'mutt -y' C-m

tmux new-window -n nginx-dev -t dev
tmux send-keys -t dev:3 'cdb nginx-se' C-m
tmux send-keys -t dev:3 'vim-enter-session develop' C-m

tmux new-window -n run -t dev
tmux send-keys -t dev:4 'cdb nginx-se' C-m

tmux new-window -n tests -t dev
tmux send-keys -t dev:5 'cdb nginx-tests-se' C-m

tmux new-window -n ffmpeg -t dev
tmux send-keys -t dev:6 'cdb ffmpeg' C-m

tmux new-window -n mediadata -t dev
tmux send-keys -t dev:7 'cdb mediadata' C-m

tmux new-window -n ns-nginx-com -t dev
tmux send-keys -t dev:8 'ssh ns.nginx.com' C-m

tmux new-window -n dev-nginx-com -t dev
tmux send-keys -t dev:9 'ssh dev.nginx.com' C-m

tmux select-window -t dev:3
fi

tmux attach -t dev
