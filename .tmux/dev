tmux has-session -t dev
if [ $? != 0 ]; then
reload-ssh-agent
tmux new-session -s dev -n shell -d

tmux new-window -n build-env-shell -t dev
tmux send-keys -t dev:2 'prepare-workspace' C-m

tmux new-window -n ide-env -t dev
tmux send-keys -t dev:3 'prepare-workspace' C-m
tmux send-keys -t dev:3 'vim-enter-dev' C-m

tmux new-window -n test-1 -t dev
tmux send-keys -t dev:4 'host-enter root@10.40.26.253' C-m
tmux send-keys -t dev:4 'prepare-workspace' C-m

tmux new-window -n test-1_2 -t dev
tmux send-keys -t dev:5 'host-enter root@10.40.26.253' C-m

tmux new-window -n test-nvenc -t dev
tmux send-keys -t dev:6 'host-enter root@10.40.25.195' C-m
tmux send-keys -t dev:6 'prepare-workspace' C-m

tmux new-window -n test-nvenc_2 -t dev
tmux send-keys -t dev:7 'host-enter root@10.40.25.195' C-m

tmux select-window -t dev:3
fi

tmux attach -t dev
